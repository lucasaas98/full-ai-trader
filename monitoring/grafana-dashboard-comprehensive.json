                "targets": [
                    {
                        "expr": "sum(trading_volume_total) by (symbol)",
                        "legendFormat": "{{symbol}}"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": { "mode": "palette-classic" },
                        "unit": "short"
                    }
                },
                "options": {
                    "reduceOptions": {
                        "values": false,
                        "calcs": ["lastNotNull"],
                        "fields": ""
                    },
                    "pieType": "pie",
                    "tooltip": { "mode": "single", "sort": "none" },
                    "legend": {
                        "displayMode": "table",
                        "placement": "right",
                        "values": ["value", "percent"]
                    },
                    "displayLabels": ["name", "percent"]
                },
                "gridPos": { "h": 8, "w": 12, "x": 12, "y": 57 }
            },
