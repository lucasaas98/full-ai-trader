                            "properties": [
                                { "id": "unit", "value": "ms" },
                                {
                                    "id": "thresholds",
                                    "value": {
                                        "steps": [
                                            { "color": "green", "value": null },
                                            { "color": "yellow", "value": 100 },
                                            { "color": "red", "value": 500 }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                "gridPos": { "h": 6, "w": 24, "x": 0, "y": 51 }
            },
            {
                "id": 11,
                "title": "Active Positions",
                "type": "table",
                "targets": [
                    {
                        "expr": "trading_position_market_value by (symbol)",
                        "legendFormat": "{{symbol}}"
                    }
                ],
                "transformations": [
                    {
                        "id": "organize",
                        "options": {
                            "excludeByName": {},
                            "indexByName": {},
                            "renameByName": {
                                "symbol": "Symbol",
                                "Value": "Market Value",
                                "trading_position_quantity": "Quantity",
                                "trading_position_unrealized_pnl": "Unrealized P&L"
                            }
                        }
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "align": "auto",
                            "displayMode": "auto",
