                    },
                    "overrides": [
                        {
                            "matcher": {
                                "id": "byName",
                                "options": "Unrealized P&L"
                            },
                            "properties": [
                                {
                                    "id": "custom.displayMode",
                                    "value": "color-background"
                                },
                                {
                                    "id": "thresholds",
                                    "value": {
                                        "steps": [
                                            { "color": "red", "value": null },
                                            {
                                                "color": "yellow",
                                                "value": -100
                                            },
                                            { "color": "green", "value": 0 }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                "gridPos": { "h": 8, "w": 12, "x": 0, "y": 57 }
            },
            {
                "id": 12,
                "title": "Trading Volume by Symbol",
