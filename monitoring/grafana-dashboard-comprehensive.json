{
    "dashboard": {
        "id": null,
        "title": "AI Trading System - Comprehensive Monitoring",
        "tags": ["trading", "ai", "monitoring", "real-time"],
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "System Overview",
                "type": "stat",
                "targets": [
                    {
                        "expr": "up{job=\"trading-system\"}",
                        "legendFormat": "Services Up"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                { "color": "red", "value": 0 },
                                { "color": "yellow", "value": 3 },
                                { "color": "green", "value": 5 }
                            ]
                        },
                        "unit": "short"
                    }
                },
                "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
            },
            {
                "id": 2,
                "title": "Portfolio Value (Real-time)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "trading_portfolio_total_value",
                        "legendFormat": "Total Portfolio Value"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "value"
                        },
                        "unit": "currencyUSD"
                    }
                },
                "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
            },
            {
                "id": 3,
                "title": "Trading Performance Metrics",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "rate(trading_trades_executed_total[5m])",
                        "legendFormat": "Trades per minute"
                    },
                    {
                        "expr": "trading_strategy_success_rate",
                        "legendFormat": "Strategy Success Rate"
                    },
                    {
                        "expr": "trading_portfolio_daily_pnl",
                        "legendFormat": "Daily P&L"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "drawStyle": "line",
                            "lineInterpolation": "linear",
                            "barAlignment": 0,
                            "lineWidth": 2,
                            "fillOpacity": 10,
                            "gradientMode": "none",
                            "spanNulls": false,
                            "insertNulls": false,
                            "showPoints": "never",
                            "pointSize": 5,
                            "stacking": { "mode": "none", "group": "A" },
                            "axisPlacement": "auto",
                            "axisLabel": "",
                            "scaleDistribution": { "type": "linear" },
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            },
                            "thresholdsStyle": { "mode": "off" }
                        },
                        "color": { "mode": "palette-classic" },
                        "unit": "short"
                    }
                },
                "options": {
                    "tooltip": { "mode": "single", "sort": "none" },
                    "legend": {
                        "displayMode": "table",
                        "placement": "bottom",
                        "calcs": ["lastNotNull", "mean", "max"]
                    }
                },
                "gridPos": { "h": 9, "w": 24, "x": 0, "y": 8 }
            },
            {
                "id": 4,
                "title": "API Response Times",
                "type": "heatmap",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(trading_api_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(trading_api_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "50th percentile"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "hideFrom": {
                                "legend": false,
                                "tooltip": false,
                                "vis": false
                            }
                        },
                        "unit": "ms"
                    }
                },
                "gridPos": { "h": 9, "w": 12, "x": 0, "y": 17 }
            },
            {
                "id": 5,
                "title": "Trade Execution Latency",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "trading_trade_execution_latency_ms",
                        "legendFormat": "Execution Latency"
                    },
                    {
                        "expr": "trading_order_fill_time_ms",
                        "legendFormat": "Order Fill Time"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": { "mode": "palette-classic" },
                        "unit": "ms",
                        "thresholds": {
                            "steps": [
                                { "color": "green", "value": null },
                                { "color": "yellow", "value": 100 },
                                { "color": "red", "value": 500 }
                            ]
                        }
                    }
                },
                "gridPos": { "h": 9, "w": 12, "x": 12, "y": 17 }
            },
            {
                "id": 6,
                "title": "Strategy Performance Comparison",
                "type": "table",
                "targets": [
                    {
                        "expr": "trading_strategy_pnl_total by (strategy_name)",
                        "legendFormat": "{{strategy_name}}"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "custom": {
                            "align": "auto",
                            "displayMode": "auto",
                            "filterable": true
                        },
                        "mappings": [],
                        "thresholds": {
                            "steps": [
                                { "color": "green", "value": null },
                                { "color": "red", "value": -1000 }
                            ]
                        },
                        "unit": "currencyUSD"
                    },
                    "overrides": [
                        {
                            "matcher": {
                                "id": "byName",
                                "options": "Strategy"
                            },
                            "properties": [
                                { "id": "custom.width", "value": 150 }
                            ]
                        }
                    ]
                },
                "options": {
                    "showHeader": true,
                    "sortBy": [{ "desc": true, "displayName": "Total P&L" }]
                },
                "gridPos": { "h": 8, "w": 24, "x": 0, "y": 26 }
            },
            {
                "id": 7,
                "title": "System Resource Usage",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "trading_system_cpu_usage_percent",
                        "legendFormat": "CPU Usage %"
                    },
                    {
                        "expr": "trading_system_memory_usage_percent",
                        "legendFormat": "Memory Usage %"
                    },
                    {
                        "expr": "trading_system_disk_usage_percent",
                        "legendFormat": "Disk Usage %"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": { "mode": "palette-classic" },
                        "unit": "percent",
                        "min": 0,
                        "max": 100,
                        "thresholds": {
                            "steps": [
                                { "color": "green", "value": null },
                                { "color": "yellow", "value": 70 },
                                { "color": "red", "value": 85 }
                            ]
                        }
                    }
                },
                "gridPos": { "h": 8, "w": 12, "x": 0, "y": 34 }
            },
            {
                "id": 8,
                "title": "Error Rate by Service",
                "type": "bargauge",
                "targets": [
                    {
                        "expr": "rate(trading_errors_total[5m]) by (service)",
                        "legendFormat": "{{service}}"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                { "color": "green", "value": null },
                                { "color": "yellow", "value": 0.01 },
                                { "color": "red", "value": 0.05 }
                            ]
                        },
                        "unit": "percent"
                    }
                },
                "gridPos": { "h": 8, "w": 12, "x": 12, "y": 34 }
            },
            {
                "id": 9,
                "title": "Risk Metrics Dashboard",
                "type": "timeseries",
                "targets": [
                    {
                        "expr": "trading_portfolio_var_95",
                        "legendFormat": "Value at Risk (95%)"
                    },
                    {
                        "expr": "trading_portfolio_sharpe_ratio",
                        "legendFormat": "Sharpe Ratio"
                    },
                    {
                        "expr": "trading_portfolio_max_drawdown",
                        "legendFormat": "Max Drawdown %"
                    },
                    {
                        "expr": "trading_portfolio_volatility",
                        "legendFormat": "Portfolio Volatility"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": { "mode": "palette-classic" },
                        "unit": "short"
                    },
                    "overrides": [
                        {
                            "matcher": {
                                "id": "byName",
                                "options": "Max Drawdown %"
                            },
                            "properties": [
                                { "id": "unit", "value": "percent" },
                                {
                                    "id": "color",
                                    "value": {
                                        "mode": "fixed",
                                        "fixedColor": "red"
                                    }
                                }
                            ]
                        },
                        {
                            "matcher": {
                                "id": "byName",
                                "options": "Value at Risk (95%)"
                            },
                            "properties": [
                                { "id": "unit", "value": "currencyUSD" }
                            ]
                        }
                    ]
                },
                "gridPos": { "h": 9, "w": 24, "x": 0, "y": 42 }
            },
            {
                "id": 10,
                "title": "Market Data Quality",
                "type": "stat",
                "targets": [
                    {
                        "expr": "trading_market_data_quality_score",
                        "legendFormat": "Data Quality Score"
                    },
                    {
                        "expr": "rate(trading_market_data_received_total[1m])",
                        "legendFormat": "Data Updates/min"
                    },
                    {
                        "expr": "trading_market_data_latency_ms",
                        "legendFormat": "Data Latency (ms)"
